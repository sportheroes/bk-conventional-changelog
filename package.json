{
  "name": "@sportheroes/bk-conventional-changelog",
  "version": "2.0.1",
  "description": "conventional-changelog SportHeroesGroup backend preset",
  "main": "index.js",
  "scripts": {
    "lint": "jshint *.js --exclude node_modules && jscs *.js",
