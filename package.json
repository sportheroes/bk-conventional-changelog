  },
  "homepage": "https://github.com/sportheroes/bk-conventional-changelog#readme",
  "devDependencies": {
    "chai": "^4.1.1",
    "conventional-changelog-core": "1.9.0",
    "git-dummy-commit": "^1.3.0",
    "jscs": "^3.0.7",
    "jshint": "^2.9.5",
    "mocha": "*",
    "shelljs": "^0.7.8",
